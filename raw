
query jingalala(username: $username, password: $password) {
  login(username: $username, password: $password) {
    status
    tokens(order: "latest", limit: 1) {
      id
    }
  }
}

mutation jingalala(username: $username, password: $password) {
  login(username: $username, password: $password) {
    status
    tokens(order: "latest", limit: 1) {
      id
    }
  }
}
